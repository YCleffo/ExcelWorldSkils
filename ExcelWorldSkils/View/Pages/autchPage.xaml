<Page x:Class="ExcelWorldSkils.Model.Frame.AutchPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExcelWorldSkils.Model.Frame"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Авторизация">

    <Grid>
        <StackPanel VerticalAlignment="Center" Width="200" HorizontalAlignment="Center">
            <TextBlock Text="Логин" Margin="6" FontSize="16"/>
            <TextBox x:Name="LogIn"/>
            <TextBlock Text="Пароль" Margin="6" FontSize="16"/>
            <TextBox x:Name="Password"/>
            <Button x:Name="AutchBtn" Click="AutchBtnClick" Content="Войти" Margin="0 6"/>
        </StackPanel>
    </Grid>
</Page>
